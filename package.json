{
  "name": "github-actions",
  "version": "0.0.1",
  "private": true,
  "description": "Helpful GitHub actions",
  "scripts": {
    "build": "rm -rf dist/* && ttsc",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "all": "npm run build && npm run format && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AHW214/github-actions.git"
  },
  "author": "Adam Weider",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "dateformat": "^4.6.1",
    "mustache": "^4.2.0",
    "pretty-bytes": "^5.6.0",
    "purify-ts": "^1.0.0"
  },
  "devDependencies": {
    "@types/dateformat": "^3.0.1",
    "@types/mustache": "^4.1.2",
    "@types/node": "^16.3.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.31.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "prettier": "2.2.1",
    "ts-transformer-imports": "^0.4.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.3"
  }
}
